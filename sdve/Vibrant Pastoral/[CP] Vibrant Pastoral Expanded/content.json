{
	"Format": "2.4.0",
	"ConfigSchema": {
		"Buildings": {
			"AllowValues": "farm, lighthouse, morris",
			"AllowMultiple": true,
			"AllowBlank": true,
			"Default": "farm, lighthouse, morris",
            "Section": "Buildings"
		},
		"PremiumBarn": {
			"AllowValues": "true, false",
			"Default": "true",
            "Section": "Farm"
		},
		"PremiumCoop": {
			"AllowValues": "true, false",
			"Default": "true",
            "Section": "Farm"
		},
		"Winery": {
			"AllowValues": "true, false",
			"Default": "true",
            "Section": "Farm"
		},
		"Fences": {
			"AllowValues": "true, false",
			"Default": "true",
			"Section": "Buildables"
		},
		"brokenSVEupdate": {
			"AllowValues": "true, false",
			"Default": "false",
            "Section": "Debug"
		}
	},
	"DynamicTokens": [
		{
			"Name": "SeasonOverride",
			"Value": "{{Season}}"
		},
		{
			"Name": "SeasonOverride",
			"Value": "Spring",
			"When": { "Query: NOT {{IsOutdoors}} OR {{LocationContext |contains=Desert}}": true}
		},
		{
			"Name": "SeasonOverride",
			"Value": "Summer",
			"When": { "LocationContext": "Island" }
		},
		{
			"Name": "SVEMap",
			"Value": "immersive2",
			"When": {
				"HasMod": "flashshifter.immersivefarm2remastered",
				"FarmType": "Standard",
				"HasMod |contains=flashshifter.GrandpasFarm": false
			}
		},
		{
			"Name": "SVEMap",
			"Value": "grandpas",
			"When": {
				"HasMod": "flashshifter.GrandpasFarm",
				"FarmType": "Standard",
				"HasMod |contains=flashshifter.immersivefarm2remastered": false
			}
		},
		{
			"Name": "TreeType",
			"Value": "Vanilla"
		},
		{
			"Name": "TreeType",
			"Value": "Simple Foliage",
			"When": { "HasMod": "Az.Foliage" }
		},
		{
			"Name": "SnOverlay",
			"Value": ""
		},
		{
			"Name": "SnOverlay",
			"Value": "{{LocationContext}}",
            "When": {
				"VibrantPastoral.C/SnOverlay": true,
				"Season": "winter"
            }
		}
	],
	"Changes": [
	//Inclusions
		//0Load
        {
            "Action": "Include",
            "FromFile": "data/0load.json",
			"When": { "brokenSVEupdate": false }
        },
		//0Main
        {
            "Action": "Include",
            "FromFile": "data/0main.json"
        },
        //Buildings:Farm
        {
            "Action": "Include",
            "FromFile": "data/buildings.json, data/masks.json",
			"When": { "Buildings |contains=farm": true }
        },
		//Fences
        {
            "Action": "Include",
            "FromFile": "data/fences.json",
			"When": { "Fences": true }
        },
		//Vegetation
        {
            "Action": "Include",
            "FromFile": "data/vegetation.json",
			"When": {
				"brokenSVEupdate": false,
				"VibrantPastoral.C/Vegetation": true
			}
        },
		//Patches
        {
            "Action": "Include",
            "FromFile": "data/patches.json"
        },
		//Furniture
        {
            "Action": "Include",
            "FromFile": "data/furniture.json",
			"When": {
				"brokenSVEupdate": false,
				"VibrantPastoral.C/Furniture": true
			}
        },
		//Interiors
        {
            "Action": "Include",
            "FromFile": "data/interiors.json",
			"When": {
				"brokenSVEupdate": false,
				"VibrantPastoral.C/Interiors": true
			}
        },
		//Frontier Farm
        {
            "Action": "Include",
            "FromFile": "data/frontier.json",
			"When": {
				"HasMod": "flashshifter.FrontierFarm",
				"FarmType": "FrontierFarm"
			}
		},
		//Grandpa's Farm | Immersive 2
        {
            "Action": "Include",
            "FromFile": "data/{{SVEMap}}.json",
			"When": { "HasFile:{{FromFile}}": true }
		},
		//Grampleton Fields
        {
            "Action": "Include",
            "FromFile": "data/grampletonfields.json",
            "When": {
				"brokenSVEupdate": false,
                "HasMod": "flashshifter.GrampletonFields",
                "HasMod |contains=LemurKat.EastScarpe.CP": false
            }
		},
		//Joja
        {
            "Action": "Include",
            "FromFile": "data/joja.json",
			"When": { "HasSeenEvent": "5553210" }
        },
		//Build on/Buildable Ginger Island
        {
            "Action": "Include",
            "FromFile": "data/compat/BuildGingerIsland.json",
			"Update": "OnLocationChange",
			"When": {
				"Buildings |contains=farm": true,
				"Query: {{HasMod |contains=Sexynes.CP.BuildOnGingerIslandFarm}} OR {{HasMod |contains=mouahrara.BuildableGingerIslandFarm}}": true,
				"LocationName": "IslandWest"
			}
        },
		//NPC Map Locations
        {
            "Action": "Include",
            "FromFile": "data/compat/NPCMapLocations.json",
			"When": { "HasMod": "Bouhm.NPCMapLocations" }
        }
	]
}